:- object(arithmetic_expressions).

	p(X) :- X == 2.0.

	q(X) :- 2.0 =:= X.

	r(X, Y) :- sqrt(X) =:= sin(Y).

	:- set_logtalk_flag(always_true_or_false_goals, silent).
	s :- 3.0 \= 3.

:- end_object.
